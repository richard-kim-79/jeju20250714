# 환경 설정
NODE_ENV=development
PORT=3001

# 데이터베이스 설정
DATABASE_URL=postgresql://username:password@localhost:5432/jeju_sns

# JWT 설정
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# 로그 설정
LOG_LEVEL=info

# 업로드 설정
UPLOAD_PATH=./uploads
MAX_FILE_SIZE=10485760

# 이메일 설정 (추후 구현)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# 외부 API 설정 (추후 구현)
WEATHER_API_KEY=your-weather-api-key
MAP_API_KEY=your-map-api-key

# 보안 설정
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100
BCRYPT_ROUNDS=12

# 개발 환경 설정
DEV_CORS_ORIGIN=http://localhost:3000,http://localhost:3001

# 베타 환경 설정
BETA_CORS_ORIGIN=https://beta.jeju-sns.com

# 프로덕션 환경 설정
PROD_CORS_ORIGIN=https://jeju-sns.com,https://www.jeju-sns.com