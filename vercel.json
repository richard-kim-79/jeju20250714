{
  "version": 2,
  "functions": {
    "api/posts.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/posts",
      "dest": "/api/posts.js"
    },
    {
      "src": "/script.js",
      "dest": "/script.js"
    },
    {
      "src": "/styles.css",
      "dest": "/styles.css"
    },
    {
      "src": "/admin.html",
      "dest": "/admin.html"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 