{
  "version": 2,
  "functions": {
    "api/posts.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/posts",
      "dest": "/api/posts.js"
    },
    {
      "src": "/admin.html",
      "dest": "/admin.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 